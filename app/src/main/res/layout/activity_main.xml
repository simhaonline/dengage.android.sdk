<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/appAliasText"
        android:layout_width="wrap_content"
        android:layout_height="12dp"
        android:text="AppAlias"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/tokenText"
        tools:layout_editor_absoluteX="348dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/tokenText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="tokenText"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/advertisingIdText"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="436dp"
        android:text="AdvertisingId"
        app:layout_constraintTop_toBottomOf="@+id/tokenText"
        tools:layout_editor_absoluteX="56dp" />


</androidx.constraintlayout.widget.ConstraintLayout>